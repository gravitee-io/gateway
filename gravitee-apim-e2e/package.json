{
    "name": "gravitee-apim-e2e",
    "version": "3.16.1",
    "private": true,
    "description": "Gravitee.io APIM - e2e",
    "scripts": {
        "test": "cypress open --config-file ui-test/integration/cypress-config.json",
        "test:apim": "cypress open --config-file ui-test/integration/apim/cypress-config.json",
        "test:apim:bulk": "cypress run --config-file ui-test/integration/apim/bulk/cypress-config.json",
        "test:apim:rest-api": "cypress run --config-file ui-test/integration/apim/rest-api/cypress-config.json",
        "test:apim:e2e": "cypress open --config-file ui-test/integration/apim/e2e/cypress-config.json",
        "test:apim:ui": "cypress open --config-file ui-test/integration/apim/ui/cypress-config.json",
        "ci:apim:ui": "cypress run --config-file ui-test/integration/apim/ui/cypress-config.json",
        "test:apim:dev": "cypress run --env printLogsToConsole=always --config-file ui-test/integration/apim/rest-api/cypress-config.json",
        "test:apim:parallel": "cypress run --record --group apis --spec 'ui-test/integration/apim/rest-api/apis/**/*' --config-file ui-test/integration/apim/rest-api/cypress-config.json",
        "test:platform": "cypress open --config-file ui-test/integration/platform/cypress-config.json",
        "test:api": "jest api-test",
        "lint": "npm run prettier && npm run lint:license",
        "lint:fix": "npm run prettier:fix && npm run lint:license:fix",
        "lint:license": "license-check-and-add check -f license-check-config.json",
        "lint:license:fix": "license-check-and-add add -f license-check-config.json -r",
        "prettier": "prettier --check \"**/*.{js,ts,html,css,scss,json}\"",
        "prettier:fix": "prettier --write \"**/*.{js,ts,html,css,scss,json}\"",
        "prettier:list": "prettier --list-different \"**/*.{js,ts,html,css,scss,json}\"",
        "update:sdk:management": "sh ./scripts/update-management-sdk.sh",
        "update:sdk:portal": "sh ./scripts/update-portal-sdk.sh"
    },
    "license": "MIT License",
    "devDependencies": {
        "@jest/create-cache-key-function": "^28.0.0",
        "@types/faker": "5.5.9",
        "@types/jest": "27.4.1",
        "@types/node": "16.10.9",
        "@types/node-fetch": "2.6.1",
        "ansi-regex": "6.0.1",
        "cypress": "9.5.0",
        "cypress-terminal-report": "3.4.1",
        "dotenv": "16.0.0",
        "har-validator": "5.1.5",
        "jest": "27.5.1",
        "jsonwebtoken": "8.5.1",
        "license-check-and-add": "4.0.3",
        "node-fetch": "2.6.7",
        "prettier": "2.5.1",
        "ts-jest": "27.1.4",
        "typescript": "4.5.2"
    },
    "dependencies": {
        "@types/jsonwebtoken": "8.5.8",
        "faker": "5.5.3"
    }
}
