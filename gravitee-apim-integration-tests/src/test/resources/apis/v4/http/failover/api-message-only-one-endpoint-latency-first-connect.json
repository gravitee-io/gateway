{
  "id": "my-api",
  "name": "my-api",
  "apiVersion": "1.0",
  "definitionVersion": "4.0.0",
  "type": "message",
  "listeners": [
    {
      "type": "http",
      "paths": [
        {
          "path": "/test"
        }
      ],
      "entrypoints": [
        {
          "type": "http-get",
          "configuration": {
            "messagesLimitCount": 5,
            "messagesLimitDurationMs": 10000,
            "headersInPayload": false,
            "metadataInPayload": false
          }
        }
      ]
    }
  ],
  "endpointGroups": [
    {
      "name": "default-group",
      "type": "mock",
      "endpoints": [
        {
          "name": "default",
          "type": "mock",
          "weight": 1,
          "inheritConfiguration": false,
          "configuration": {
            "messageInterval": 100,
            "messageCount": 5,
            "messageContent": "{ \"message\": \"hello\" }",
            "connectionLatencies": [600]
          }
        }
      ]
    }
  ],
  "failover": {
    "enabled": true,
    "maxRetries": 2,
    "slowCallDuration": 500,
    "maxFailures": 2,
    "perSubscription": false
  },
  "flows": [
    {
      "name": "flow-1",
      "enabled": true,
      "request": [],
      "response": [],
      "subscribe": [],
      "publish": [
      ]
    }
  ],
  "analytics": {
    "enabled": false
  }
}
