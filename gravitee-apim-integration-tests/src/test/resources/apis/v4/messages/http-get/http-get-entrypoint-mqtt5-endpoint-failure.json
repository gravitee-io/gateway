{
  "id": "http-get-endpoint-mqtt5-endpoint-auto",
  "name": "my-api",
  "apiVersion": "1.0",
  "definitionVersion": "4.0.0",
  "type": "message",
  "description": "api v4 using HTTP Get entrypoint",
  "listeners": [
    {
      "type": "http",
      "paths": [
        {
          "path": "/test-failure"
        }
      ],
      "entrypoints": [
        {
          "type": "http-get",
          "configuration": {
            "messagesLimitCount": 2,
            "messagesLimitDurationMs": 10000,
            "headersInPayload": true,
            "metadataInPayload": true
          }
        }
      ]
    }
  ],
  "endpointGroups": [
    {
      "name": "default-group",
      "type": "mqtt5",
      "endpoints": [
        {
          "name": "default",
          "type": "mqtt5",
          "weight": 1,
          "inheritConfiguration": false,
          "configuration": {
            "serverHost": "wrong",
            "serverPort": "1234"
          },
          "sharedConfigurationOverride": {
            "consumer": {
              "enabled": true,
              "topic": "test-topic"
            }
          }
        }
      ]
    }
  ],
  "flows": [],
  "analytics": {
    "enabled": false
  }
}
