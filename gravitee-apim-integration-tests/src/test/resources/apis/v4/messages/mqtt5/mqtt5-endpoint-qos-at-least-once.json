{
  "id": "mqtt5-endpoint-qos-at-least-once",
  "name": "my-api",
  "apiVersion": "1.0",
  "definitionVersion": "4.0.0",
  "type": "message",
  "description": "api v4 using HTTP Get entrypoint",
  "listeners": [
    {
      "type": "subscription",
      "entrypoints": [
        {
          "type": "webhook"
        }
      ]
    },
    {
      "type": "http",
      "paths": [
        {
          "path": "/test-qos-at-least-once"
        }
      ],
      "entrypoints": [
        {
          "type": "http-get",
          "qos": "at-least-once",
          "configuration": {
            "messagesLimitCount": 1,
            "messagesLimitDurationMs": 10000,
            "headersInPayload": true,
            "metadataInPayload": true
          }
        },
        {
          "type": "sse",
          "qos": "at-least-once",
          "configuration": {}
        }
      ]
    }
  ],
  "endpointGroups": [
    {
      "name": "default-group",
      "type": "mqtt5",
      "endpoints": [
        {
          "name": "default",
          "type": "mqtt5",
          "weight": 1,
          "inheritConfiguration": false,
          "configuration": {
            "serverHost": "localhost",
            "serverPort": "mqtt5-port"
          },
          "sharedConfigurationOverride": {
            "consumer": {
              "enabled": true,
              "topic": "test-topic-qos-at-least-once"
            },
            "producer": {
              "enabled": true,
              "topic": "test-topic-qos-at-least-once"
            }
          }
        }
      ]
    }
  ],
  "flows": [
    {
      "name": "Flow ready",
      "enabled": true,
      "subscribe": [
        {
          "name": "Message Flow Ready",
          "description": "Detect the message flow is ready",
          "enabled": true,
          "policy": "message-flow-ready"
        }
      ]
    }
  ],
  "analytics": {
    "enabled": false
  }
}
