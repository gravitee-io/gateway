{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "checkJs": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./dist",
    "removeComments": false,
    "skipLibCheck": true,
    "strict": false,
    "target": "es5",
    "types": ["./types", "node"],
    "paths": {
      "@management-fakers/*": ["lib/fixtures/management/*"],
      "@management-apis/*": ["lib/management-webclient-sdk/src/lib/apis/*"],
      "@client-conf/*": ["lib/configuration"],
      "@management-models/*": ["lib/management-webclient-sdk/src/lib/models/*"]
    }
  },
  "include": ["**/*.ts"]
}
