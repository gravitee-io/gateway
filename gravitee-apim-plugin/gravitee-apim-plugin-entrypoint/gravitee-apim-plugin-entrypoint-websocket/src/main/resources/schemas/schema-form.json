{
    "type": "object",
    "id": "urn:jsonschema:io:gravitee:plugin:entrypoint:webhook:configuration:WebsocketEntrypointConnectorConfiguration",
    "properties": {
        "publisher": {
            "type": "object",
            "title": "Publisher configuration",
            "id": "urn:jsonschema:io:gravitee:plugin:entrypoint:webhook:configuration:WebsocketEntrypointConnectorConfiguration:Publisher",
            "properties": {
                "enabled": {
