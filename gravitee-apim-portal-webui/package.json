{
  "name": "@gravitee/portal-webui",
  "version": "0.0.0",
  "description": "Gravitee.io Portal",
  "scripts": {
    "build": "NODE_OPTIONS=--max_old_space_size=8192 ng build",
    "build:prod": "NODE_OPTIONS=--max_old_space_size=8192 ng build --configuration production",
    "e2e": "ng e2e",
    "generate:i18n": "ngx-translate-extract -i ./src -o ./src/assets/i18n/*.json -c -s --fi '  ' -f namespaced-json -m i18n",
    "lint": "ng lint && npm run prettier",
    "lint:fix": "ng lint --fix && npm run prettier:fix && npm run lint:license:fix",
    "lint:license": "license-check-and-add check -f license-check-config.json",
    "lint:license:fix": "license-check-and-add add -f license-check-config.json -r",
    "ng": "ng",
    "prettier": "prettier --check \"**/*.{js,ts,html,css,scss,json}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts,html,css,scss,json}\"",
    "prettier:list": "prettier --list-different \"**/*.{js,ts,html,css,scss,json}\"",
    "serve": "NODE_OPTIONS=--max_old_space_size=8192 ng serve --port=4100",
    "serve:apim-master": "NODE_OPTIONS=--max_old_space_size=8192 ng run gravitee-portal-webui:serve:apim-master --port=4100",
    "serve:prod": "node scripts/serve.js",
    "test": "NODE_OPTIONS=--max-old-space-size=3072 ng test -w 8",
    "test:coverage": "NODE_OPTIONS=--max-old-space-size=3072 ng test --collect-coverage -w 8",
    "test:watch": "NODE_OPTIONS=--max-old-space-size=3072 ng test --watch -w 8",
    "update:sdk": "sh ./scripts/update-sdk.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.3.0",
    "@angular/common": "14.3.0",
    "@angular/compiler": "14.3.0",
    "@angular/core": "14.3.0",
    "@angular/forms": "14.3.0",
    "@angular/localize": "14.3.0",
    "@angular/platform-browser": "14.3.0",
    "@angular/platform-browser-dynamic": "14.3.0",
    "@angular/router": "14.3.0",
    "@asyncapi/web-component": "1.0.0-next.47",
    "@fontsource/ibm-plex-sans": "4.5.5",
    "@gravitee/ui-components": "3.44.0",
    "@highcharts/map-collection": "2.0.0",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "angular-oauth2-oidc": "13.0.1",
    "asciidoctor": "2.2.6",
    "asciidoctor-highlight.js": "0.4.0",
    "clipboard-copy": "4.0.1",
    "highcharts": "10.0.0",
    "highlight.js": "11.5.0",
    "jdenticon": "3.1.1",
    "js-yaml": "4.1.0",
    "marked": "4.3.0",
    "messageformat": "2.3.0",
    "ngx-cookie-service": "14.0.1",
    "ngx-translate-messageformat-compiler": "5.1.0",
    "redoc": "2.0.0",
    "resize-observer-polyfill": "1.5.1",
    "rxjs": "6.5.5",
    "swagger-ui-dist": "4.3.0",
    "tslib": "2.3.1",
    "url": "0.11.0",
    "zone.js": "0.11.5"
  },
  "engines": {
    "node": "^18.18",
    "npm": "^9.8"
  },
  "devDependencies": {
    "@angular-builders/jest": "14.1.0",
    "@angular-devkit/build-angular": "14.2.13",
    "@angular-devkit/core": "14.2.13",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "14.2.13",
    "@angular/compiler-cli": "14.3.0",
    "@angular/language-service": "14.3.0",
    "@babel/preset-env": "7.16.11",
    "@biesbjerg/ngx-translate-extract": "7.0.4",
    "@biesbjerg/ngx-translate-extract-marker": "1.0.0",
    "@ngneat/spectator": "10.0.0",
    "@types/jest": "28.1.8",
    "@types/js-yaml": "4.0.5",
    "@types/marked": "4.0.3",
    "@types/node": "16.10.9",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "@webcomponents/webcomponentsjs": "2.6.0",
    "babel-jest": "28.1.3",
    "compression": "1.7.4",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "http-proxy-middleware": "1.3.1",
    "jest": "28.1.3",
    "jest-preset-angular": "^12.2.6",
    "license-check-and-add": "4.0.3",
    "ng-packagr": "^14.2.2",
    "prettier": "2.8.4",
    "protractor": "7.0.0",
    "ts-node": "8.10.2",
    "typescript": "4.6.4"
  }
}
